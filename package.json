{
  "name": "@jict/changelogithub",
  "type": "module",
  "version": "0.12.7",
  "packageManager": "pnpm@7.2.0",
  "description": "Generate changelog for GitHub.",
  "keywords": [
    "github",
    "release",
    "releases",
    "conventional",
    "changelog",
    "log"
  ],
  "license": "MIT",
  "author": "Anthony Fu <anthonyfu117@hotmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antfu/changelogithub.git"
  },
  "funding": "https://github.com/sponsors/antfu",
  "homepage": "https://github.com/antfu/changelogithub#readme",
  "bugs": "https://github.com/antfu/changelogithub/issues",
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "files": [
    "dist",
    "*.mjs"
  ],
  "bin": "./cli.mjs",
  "sideEffects": false,
  "engines": {
    "node": ">=12.0.0"
  },
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "test": "vitest",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp --commit --push --tag",
    "start": "nr dev && node cli.mjs",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@antfu/utils": "^0.7.2",
    "c12": "^1.1.0",
    "cac": "^6.7.14",
    "changelogen": "0.4.0",
    "convert-gitmoji": "0.1.3",
    "execa": "^6.1.0",
    "kolorist": "^1.6.0",
    "octokit": "^2.0.14",
    "ohmyfetch": "^0.4.21"
  },
  "devDependencies": {
    "@hannoeru/eslint-config": "^0.9.4",
    "@types/conventional-changelog": "^3.1.1",
    "@types/debug": "^4.1.7",
    "@types/fs-extra": "^9.0.13",
    "@types/minimist": "^1.2.2",
    "@types/semver": "^7.3.13",
    "bumpp": "^8.2.1",
    "eslint": "^8.29.0",
    "esno": "^0.16.3",
    "fs-extra": "^11.1.0",
    "typescript": "^4.9.4",
    "unbuild": "^1.0.2",
    "vitest": "^0.25.8"
  }
}
